{
  "name": "ukulele-world",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "start-dev": "set NODE_ENV=development && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=images/favicon.png --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=images/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Ukulele world\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=images/favicon.png --prune=true --out=release-builds",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "package": "npm-run-all package:*",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=images/favicon.ico",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js"
  },
  "author": "Olga Bovshyk",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "devtron": "^1.3.0",
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.2.2",
    "electron-winstaller": "^2.2.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "signcode": "^1.0.0",
    "spectron": "~3.6.0",
    "standard": "^8.2.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bootstrap": "^3.3.7",
    "crypto": "^1.0.1",
    "fs": "0.0.1-security",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1"
  }
}
